<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
<LinearLayout
    android:layout_width="700dp"
    android:layout_height="80dp"
    android:background="@mipmap/progressbar_background"
    android:orientation="horizontal">
<example.jni.com.coffeeseller.views.customviews.MutiProgress
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="25dp"
    android:layout_marginBottom="27dp"
    android:layout_marginLeft="25dp"
    android:layout_marginRight="25dp"
    app:nodesNum="3"
    app:progresSuccDrawable="@mipmap/ic_launcher"
    app:unprogressingDrawable="@mipmap/ic_launcher"
    app:progresFailDrawable="@mipmap/ic_launcher"
    app:progressingDrawable="@mipmap/ic_launcher"
    app:processingLineColor="#FD6F02"
    app:currNodeNO="1"/>
</LinearLayout>
</LinearLayout>
